{
  "manifest_version": 3,
  "name": "Console Logger Live",
  "version": "1.0",
  "description": "Logs all console errors, warnings and failed requests.",
  "permissions": [
    "scripting",
    "tabs"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "logger_live.js"
  },
  "action": {
    "default_title": "Console Logger Live"
  }
}